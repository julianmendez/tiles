
directive lean
import Soda.tiles.fairness.tool.TileMessage
import Soda.tiles.fairness.tile.AllEqual1Tile

/**
 * This is a composite tile for the group fairness pipeline.
 */

class AllAgentFilterNotPLengthTile

  abstract
    protected_attribute : Agent -> Boolean

  not_protected_attribute (a : Agent) : Boolean =
    not (protected_attribute (a) )

  other_tile = AllAgentFilterPLengthTile .mk (not_protected_attribute)

  apply (message : TileMessage [Boolean] ) : TileMessage [Measure] =
    other_tile .apply (
      message
    )

end

