
directive lean
import Soda.tiles.fairness.tool.TileMessage


class AtLeastTile

  abstract

  apply (message : TileMessage [Seq [TilePair [Measure] [Measure] ] ] )
    : TileMessage [Boolean] =
    TileMessageBuilder .mk .build (message .context) (message .outcome) (
      ( (message .contents)
        .map (lambda pair -->
          (Comparator .mk
            .compareMeasure (pair .fst) (pair .snd) ) >= 0 )
        .forall (lambda e --> e)
      )
    )

end

